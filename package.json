{
  "name": "form-attribution",
  "version": "2.5.0",
  "description": "Automatically capture and persist marketing attribution data in your web forms.",
  "homepage": "https://github.com/flash-brew-digital/form-attribution#readme",
  "sideEffects": false,
  "type": "module",
  "main": "dist/script.min.js",
  "files": [
    "dist/script.min.js",
    "dist/debug.min.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flash-brew-digital/form-attribution.git"
  },
  "keywords": [
    "forms",
    "lead-generation",
    "attribution",
    "marketing",
    "utm",
    "analytics",
    "crm"
  ],
  "author": {
    "name": "Ben Sabic",
    "url": "https://bensabic.ca"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/flash-brew-digital/form-attribution/issues"
  },
  "scripts": {
    "dev": "npx serve public",
    "build": "node scripts/build.js",
    "minify-assets": "node scripts/minify.js",
    "release": "pnpm build && npm publish --access public",
    "check": "npx ultracite check",
    "fix": "npx ultracite fix",
    "test": "npx playwright test",
    "bump-deps": "npx npm-check-updates -u -p pnpm && pnpm install"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@playwright/test": "^1.57.0",
    "@types/node": "^25.0.3",
    "ultracite": "7.0.10"
  },
  "packageManager": "pnpm@10.27.0"
}
